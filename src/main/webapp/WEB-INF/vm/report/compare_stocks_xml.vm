<?xml version="1.0"?>
<data>
	<code>
		#foreach($code in $codeList)
			<result name="$!{code}"></result>
    	#end
    </code>
	<rise>
    	#foreach($date in $dateList)
        <result day="$!{date}">
    		#set($i = 0)
    		#foreach($code in $codeList)
    			#set($dateRiseMap = $codeDateRiseMap.get($code)) 
    			<$!{code}>$dateRiseMap.get($date)</$!{code}>
    			#set($i = $i + 1)
    		#end
        </result>
    	#end
	</rise>
	<test>
		<result day="2011-1-1">
            <apple>81768</apple>
            <orange>60310</orange>
            <banana>43357</banana>
        </result>
        <result day="2011-1-2">
            <apple>81156</apple>
            <orange>58883</orange>
            <banana>59280</banana>
        </result>
        <result day="2011-1-3">
            <apple>72768</apple>
            <orange>63031</orange>
            <banana>41357</banana>
        </result>
        <result day="2011-1-4">
            <apple>85156</apple>
            <orange>54883</orange>
            <banana>47280</banana>
        </result>
	</test>
</data>
